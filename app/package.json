{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "2.1.1",
    "ttag": "^1.4.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "update-uk": "ttag update i18n/uk.po src/",
    "build-en": "PUBLIC_URL=/CRA-precompile-example npm run build",
    "build-uk": "PUBLIC_URL=/CRA-precompile-example/uk APP_LANG=uk npm run build",
    "build-all": "npm run build-uk && mv build uk && npm run build-en && mv uk build/",
    "predeploy": "npm run build-all",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "ttag-cli": "^1.4.2",
    "ttag.macro": "^1.4.2"
  }
}
